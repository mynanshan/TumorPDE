    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Patient Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/examples/patient-reconstruct",
            "args": ["-p", "LY", "-t", "1", "-s", "0", "-m", "1"]
        }
    ]
}